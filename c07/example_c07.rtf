#include <stdlib.h>\
\
char	*ft_strdup(char *src)\
\{\
	int		len;\
	int		i;\
	char	*dest;\
\
	len = 0;\
	while (src[len] != '\\0')\
	\{\
		len++;\
	\}\
	dest = (char *)malloc(sizeof(char) * (len + 1));\
	if (dest == NULL)\
	\{\
		return (NULL);\
	\}\
	i = 0;\
	while (src[i] != '\\0')\
	\{\
		dest[i] = src[i];\
		i++;\
	\}\
	dest[i] = '\\0';\
	return (dest);\
\}\
